{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["pdfjs-dist"],
        "lib": [
            "ES2020",
            "DOM"
        ]
    },
    "include": [
        "src/**/*"
    ]
}
